{
    "name": "songbox",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "env-cmd -f ./.env node ./bin/www",
        "dev": "env-cmd -f ./.env nodemon ./bin/www",
        "lint": "eslint --ext .js .",
        "lint:fix": "eslint --fix --ext .js .",
        "test": "env-cmd -f ./test.env jest"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "npm run lint:fix",
            "git add"
        ]
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dropbox": "^6.0.2",
        "env-cmd": "^10.1.0",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "morgan": "~1.9.1",
        "node-sass-middleware": "^0.11.0",
        "pg": "^8.5.1",
        "pug": "2.0.0-beta11",
        "supertest": "^6.0.1"
    },
    "devDependencies": {
        "eslint": "^7.11.0",
        "gulp": "^4.0.2",
        "husky": "^4.3.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.4.2",
        "nodemon": "^2.0.6"
    }
}
